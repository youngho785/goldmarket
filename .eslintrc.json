{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "react/prop-types": "off",
      "no-empty": "warn"
    },
    "ignorePatterns": [
      "node_modules/",
      "build/",
      "public/"
    ]
  }
  